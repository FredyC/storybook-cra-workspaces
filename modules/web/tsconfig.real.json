{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~web/*": ["./src/*"],
      "~common/*": ["../common/src/*"]
    },
    "lib": ["dom", "dom.iterable", "ESNext"],
    "noEmit": true,
    "jsx": "react",
    "skipLibCheck": true,
